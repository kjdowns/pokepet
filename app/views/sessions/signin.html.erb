


<div class="session font-italic text-center">
<div class=" create-account font-weight-bold">
    <h2 class="display-2 mb-5">Log In</h1>
    <%= form_tag '/signin' do %>
    Username: <%= text_field_tag :user_name, "", class:"my-2"  %><br>
    Password: <%= password_field_tag :password, "", class:"my-2"  %><br>
    <%= submit_tag "Log In", class:"btn-lg btn-secondary my-3" %>
<% end %>
</div>
</div>

<%= link_to('Log in with Google!', '/auth/google_oauth2') %>